#include<stdio.h>
#define WMAX 20
int main(void)
{
    int wordNum;
    int wordCount;
    char * words[WMAX];
    printf("请输入单词数：\n");
    scanf("%d",&wordNum);
    wordCount=wordNum;
    printf("请输入生成密码所用单词：\n");
    for(;wordNum!=0;wordNum--)
    {
        printf("剩余%d个:",wordNum);
        scanf("%s",&words[wordNum]);
    }
    printf("请输入所需密码的最大长度：")
}
