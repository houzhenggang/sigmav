unix_mkipk:
	@ if [ -e /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/data ]; then echo "ERROR: /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/data exists!"; exit 1; fi; if [ -e /root/xinlan/hardware/led/arm/ipkg/qpe-led__arm.ipk ]; then echo "Package qpe-led__arm.ipk already exists!"; exit 0; fi; IPK_GROUP_PROJECTS="/root/xinlan/hardware/led/arm"; for i in $${IPK_GROUP_PROJECTS};do make -C $$i -f Makefile unix_mkipk_deps INSTALL_ROOT=/root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/data/opt/Qtopia || exit $$?; done; make_ipk_file() { cd /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/data; if [ ! -z "$$1" ]; then lang="$$1"; suffix="_i18n_$$lang"; deps="qpe-led"; if [ "`echo qpe-led | cut -c -4`" = "qpe-" -a "qpe-led" != "qpe-taskbar" ]; then deps="$$deps qpe-taskbar_i18n_$$lang"; fi; path="opt/Qtopia/i18n/$$lang"; if [ ! -d "/root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/data/$$path" ]; then return; fi; else suffix=; deps=`cat /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/control/DEPS`; deps=`echo $$deps | sort -u`; path="opt/Qtopia"; fi; echo "Package: qpe-led$$suffix" > /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/control/control; echo "Maintainer: Unspecified" >> /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/control/control; echo "License: GPL" >> /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/control/control; echo "Version: " >> /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/control/control; echo "Arch: arm" >> /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/control/control; echo "Architecture: arm" >> /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/control/control; echo "Description: No description written for this package" >> /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/control/control; echo -n "Depends: " >> /root/xinlan/hardware/led/arm/