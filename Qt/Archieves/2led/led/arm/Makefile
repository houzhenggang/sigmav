####### Install

install_desktop: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/apps/Applications/" || $(MKDIR) "$(INSTALL_ROOT)/apps/Applications/"
	-$(INSTALL_FILE) "led.desktop" "$(INSTALL_ROOT)/apps/Applications/"
	-arm-linux-strip "$(INSTALL_ROOT)/apps/Applications/led.desktop"


uninstall_desktop: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/apps/Applications/led.desktop"
	-$(DEL_DIR) "$(INSTALL_ROOT)/apps/Applications/"


install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/bin/" || $(MKDIR) "$(INSTALL_ROOT)/bin/"
	-$(INSTALL_FILE) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/bin/$(QMAKE_TARGET)"
	-arm-linux-strip "$(INSTALL_ROOT)/bin/$(QMAKE_TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/bin/"


install_auto_pics: all 
	@ /opt/FriendlyARM/mini2440/arm-qtopia/qtopia-2.2.0-FriendlyARM/qtopia/bin/installpic "en_US" 14 16 22 $(INSTALL_ROOT)/pics/Games pics/* ;


install: install_desktop install_target install_auto_pics  

uninstall: uninstall_desktop uninstall_target  

