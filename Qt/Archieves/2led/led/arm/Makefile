g in $${LANGUAGES}; do make_ipk_file $$lang; done; [ -d /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/data/opt/Qtopia/i18n ] && rm -r /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led/data/opt/Qtopia/i18n; make_ipk_file; rm -rf /root/xinlan/hardware/led/arm/.packaging-working-dir/ipk/qpe-led

