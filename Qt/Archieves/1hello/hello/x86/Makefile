unix_mkipk:
	@ if [ -e /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/data ]; then echo "ERROR: /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/data exists!"; exit 1; fi; if [ -e /root/xinlan/software/hello/x86/ipkg/qpe-template__x86.ipk ]; then echo "Package qpe-template__x86.ipk already exists!"; exit 0; fi; IPK_GROUP_PROJECTS="/root/xinlan/software/hello/x86"; for i in $${IPK_GROUP_PROJECTS};do make -C $$i -f Makefile unix_mkipk_deps INSTALL_ROOT=/root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/data/opt/Qtopia || exit $$?; done; make_ipk_file() { cd /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/data; if [ ! -z "$$1" ]; then lang="$$1"; suffix="_i18n_$$lang"; deps="qpe-template"; if [ "`echo qpe-template | cut -c -4`" = "qpe-" -a "qpe-template" != "qpe-taskbar" ]; then deps="$$deps qpe-taskbar_i18n_$$lang"; fi; path="opt/Qtopia/i18n/$$lang"; if [ ! -d "/root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/data/$$path" ]; then return; fi; else suffix=; deps=`cat /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/DEPS`; deps=`echo $$deps | sort -u`; path="opt/Qtopia"; fi; echo "Package: qpe-template$$suffix" > /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; echo "Maintainer: Unspecified" >> /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; echo "License: GPL" >> /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; echo "Version: " >> /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; echo "Arch: x86" >> /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; echo "Architecture: x86" >> /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; echo "Description: No description written for this package" >> /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; echo -n "Depends: " >> /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; echo $$deps >> /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; echo -n "Files:" >> /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; find $$path -type f -o -type l -printf "/%P " >> /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; if [ ! -x "`which mkfs.jffs2`" ]; then echo -e "\nInstalled-Size: $$(du -h -s $$path | awk '{print $$1}')" >> /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; else echo -e "\nInstalled-Size: $$(mkfs.jffs2 -r $$path | wc -c)" >> /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; fi; echo >> /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control/control; for i in preinst prerm postinst postrm;do export FILE_LOC=/root/xinlan/software/hello/x86/qpe-template.$$i; if [ -e $$FILE_LOC ]; then install -c $$FILE_LOC /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/$$i; fi; done; cd /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template; echo "2.0" > debian-binary; cd /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control; tar --remove-files -cz --owner=0 --group=0 -f /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/control.tar.gz ./control; cd /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/data; tar --remove-files -cz --owner=0 --group=0 -f /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/data.tar.gz $$path; if [ ! -e /root/xinlan/software/hello/x86/ipkg ];then mkdir -p /root/xinlan/software/hello/x86/ipkg;fi; cd /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template; tar --remove-files -cz --owner=0 --group=0 -f /root/xinlan/software/hello/x86/ipkg/qpe-template__x86$$suffix.ipk ./debian-binary ./control.tar.gz ./data.tar.gz `for i in preinst prerm postinst postrm;do if [ -e $$i ]; then echo $$i; fi; done`; }; LANGUAGES="en_US"; for lang in $${LANGUAGES}; do make_ipk_file $$lang; done; [ -d /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/data/opt/Qtopia/i18n ] && rm -r /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template/data/opt/Qtopia/i18n; make_ipk_file; rm -rf /root/xinlan/software/hello/x86/.packaging-working-dir/ipk/qpe-template

