.obj/release-shared/main.o: main.cpp menu.h \
		mypage.h \
		qextserialport.h \
		win_qextserialport.h \
		qextserialbase.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/main.o main.cpp

.obj/release-shared/menu.o: menu.cpp menu.h \
		mypage.h \
		qextserialport.h \
		win_qextserialport.h \
		qextserialbase.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/menu.o menu.cpp

.obj/release-shared/mypage.o: mypage.cpp mypage.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/mypage.o mypage.cpp

.obj/release-shared/qextserialbase.o: qextserialbase.cpp qextserialbase.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/qextserialbase.o qextserialbase.cpp

.obj/release-shared/win_qextserialport.o: win_qextserialport.cpp win_qextserialport.h \
		qextserialbase.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/win_qextserialport.o win_qextserialport.cpp

.ui/release-shared/mymenu.h: mymenu.ui 
	$(UIC) mymenu.ui -o .ui/release-shared/mymenu.h

.ui/release-shared/mymenu.cpp: .ui/release-shared/mymenu.h mymenu.ui 
	$(UIC) mymenu.ui -i mymenu.h -o .ui/release-shared/mymenu.cpp

.obj/release-shared/mymenu.o: .ui/release-shared/mymenu.cpp .ui/release-shared/mymenu.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/mymenu.o .ui/release-shared/mymenu.cpp

.obj/release-shared/moc_menu.o: .moc/release-shared/moc_menu.cpp menu.h mypage.h \
		qextserialport.h \
		win_qextserialport.h \
		qextserialbase.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_menu.o .moc/release-shared/moc_menu.cpp

.obj/release-shared/moc_mypage.o: .moc/release-shared/moc_mypage.cpp mypage.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_mypage.o .moc/release-shared/moc_mypage.cpp

.obj/release-shared/moc_qextserialbase.o: .moc/release-shared/moc_qextserialbase.cpp qextserialbase.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_qextserialbase.o .moc/release-shared/moc_qextserialbase.cpp

.obj/release-shared/moc_win_qextserialport.o: .moc/release-shared/moc_win_qextserialport.cpp win_qextserialport.h qextserialbase.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_win_qextserialport.o .moc/release-shared/moc_win_qextserialport.cpp

.obj/release-shared/moc_mymenu.o: .moc/release-shared/moc_mymenu.cpp .ui/release-shared/mymenu.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_mymenu.o .moc/release-shared/moc_mymenu.cpp

.moc/release-shared/moc_menu.cpp: $(MOC) menu.h
	$(MOC) menu.h -o .moc/release-shared/moc_menu.cpp

.moc/release-shared/moc_mypage.cpp: $(MOC) mypage.h
	$(MOC) mypage.h -o .moc/release-shared/moc_mypage.cpp

.moc/release-shared/moc_qextserialbase.cpp: $(MOC) qextserialbase.h
	$(MOC) qextserialbase.h -o .moc/release-shared/moc_qextserialbase.cpp

.moc/release-shared/moc_win_qextserialport.cpp: $(MOC) win_qextserialport.h
	$(MOC) win_qextserialport.h -o .moc/release-shared/moc_win_qextserialport.cpp

