unix_mkipk:
	@ if [ -e /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/data ]; then echo "ERROR: /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/data exists!"; exit 1; fi; if [ -e /root/Document/mymenu/ipkg/qpe-mymenu__x86.ipk ]; then echo "Package qpe-mymenu__x86.ipk already exists!"; exit 0; fi; IPK_GROUP_PROJECTS="/root/Document/mymenu"; for i in $${IPK_GROUP_PROJECTS};do make -C $$i -f Makefile unix_mkipk_deps INSTALL_ROOT=/root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/data/opt/Qtopia || exit $$?; done; make_ipk_file() { cd /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/data; if [ ! -z "$$1" ]; then lang="$$1"; suffix="_i18n_$$lang"; deps="qpe-mymenu"; if [ "`echo qpe-mymenu | cut -c -4`" = "qpe-" -a "qpe-mymenu" != "qpe-taskbar" ]; then deps="$$deps qpe-taskbar_i18n_$$lang"; fi; path="opt/Qtopia/i18n/$$lang"; if [ ! -d "/root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/data/$$path" ]; then return; fi; else suffix=; deps=`cat /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/DEPS`; deps=`echo $$deps | sort -u`; path="opt/Qtopia"; fi; echo "Package: qpe-mymenu$$suffix" > /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; echo "Maintainer: Unspecified" >> /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; echo "License: GPL" >> /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; echo "Version: " >> /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; echo "Arch: x86" >> /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; echo "Architecture: x86" >> /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; echo "Description: No description written for this package" >> /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; echo -n "Depends: " >> /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; echo $$deps >> /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; echo -n "Files:" >> /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; find $$path -type f -o -type l -printf "/%P " >> /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; if [ ! -x "`which mkfs.jffs2`" ]; then echo -e "\nInstalled-Size: $$(du -h -s $$path | awk '{print $$1}')" >> /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; else echo -e "\nInstalled-Size: $$(mkfs.jffs2 -r $$path | wc -c)" >> /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; fi; echo >> /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control/control; for i in preinst prerm postinst postrm;do export FILE_LOC=/root/Document/mymenu/qpe-mymenu.$$i; if [ -e $$FILE_LOC ]; then install -c $$FILE_LOC /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/$$i; fi; done; cd /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu; echo "2.0" > debian-binary; cd /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control; tar --remove-files -cz --owner=0 --group=0 -f /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/control.tar.gz ./control; cd /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/data; tar --remove-files -cz --owner=0 --group=0 -f /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/data.tar.gz $$path; if [ ! -e /root/Document/mymenu/ipkg ];then mkdir -p /root/Document/mymenu/ipkg;fi; cd /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu; tar --remove-files -cz --owner=0 --group=0 -f /root/Document/mymenu/ipkg/qpe-mymenu__x86$$suffix.ipk ./debian-binary ./control.tar.gz ./data.tar.gz `for i in preinst prerm postinst postrm;do if [ -e $$i ]; then echo $$i; fi; done`; }; LANGUAGES="en_US"; for lang in $${LANGUAGES}; do make_ipk_file $$lang; done; [ -d /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/data/opt/Qtopia/i18n ] && rm -r /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu/data/opt/Qtopia/i18n; make_ipk_file; rm -rf /root/Document/mymenu/.packaging-working-dir/ipk/qpe-mymenu

