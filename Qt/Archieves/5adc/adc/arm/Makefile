unix_mkipk:
	@ if [ -e /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/data ]; then echo "ERROR: /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/data exists!"; exit 1; fi; if [ -e /opt/xinlan/hardware/adc/arm/ipkg/qpe-adc__arm.ipk ]; then echo "Package qpe-adc__arm.ipk already exists!"; exit 0; fi; IPK_GROUP_PROJECTS="/opt/xinlan/hardware/adc/arm"; for i in $${IPK_GROUP_PROJECTS};do make -C $$i -f Makefile unix_mkipk_deps INSTALL_ROOT=/opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/data/opt/Qtopia || exit $$?; done; make_ipk_file() { cd /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/data; if [ ! -z "$$1" ]; then lang="$$1"; suffix="_i18n_$$lang"; deps="qpe-adc"; if [ "`echo qpe-adc | cut -c -4`" = "qpe-" -a "qpe-adc" != "qpe-taskbar" ]; then deps="$$deps qpe-taskbar_i18n_$$lang"; fi; path="opt/Qtopia/i18n/$$lang"; if [ ! -d "/opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/data/$$path" ]; then return; fi; else suffix=; deps=`cat /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/DEPS`; deps=`echo $$deps | sort -u`; path="opt/Qtopia"; fi; echo "Package: qpe-adc$$suffix" > /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; echo "Maintainer: Unspecified" >> /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; echo "License: GPL" >> /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; echo "Version: " >> /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; echo "Arch: arm" >> /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; echo "Architecture: arm" >> /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; echo "Description: No description written for this package" >> /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; echo -n "Depends: " >> /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; echo $$deps >> /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; echo -n "Files:" >> /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; find $$path -type f -o -type l -printf "/%P " >> /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; if [ ! -x "`which mkfs.jffs2`" ]; then echo -e "\nInstalled-Size: $$(du -h -s $$path | awk '{print $$1}')" >> /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; else echo -e "\nInstalled-Size: $$(mkfs.jffs2 -r $$path | wc -c)" >> /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; fi; echo >> /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control/control; for i in preinst prerm postinst postrm;do export FILE_LOC=/opt/xinlan/hardware/adc/arm/qpe-adc.$$i; if [ -e $$FILE_LOC ]; then install -c $$FILE_LOC /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/$$i; fi; done; cd /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc; echo "2.0" > debian-binary; cd /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control; tar --remove-files -cz --owner=0 --group=0 -f /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/control.tar.gz ./control; cd /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/data; tar --remove-files -cz --owner=0 --group=0 -f /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/data.tar.gz $$path; if [ ! -e /opt/xinlan/hardware/adc/arm/ipkg ];then mkdir -p /opt/xinlan/hardware/adc/arm/ipkg;fi; cd /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc; tar --remove-files -cz --owner=0 --group=0 -f /opt/xinlan/hardware/adc/arm/ipkg/qpe-adc__arm$$suffix.ipk ./debian-binary ./control.tar.gz ./data.tar.gz `for i in preinst prerm postinst postrm;do if [ -e $$i ]; then echo $$i; fi; done`; }; LANGUAGES="en_US"; for lang in $${LANGUAGES}; do make_ipk_file $$lang; done; [ -d /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/data/opt/Qtopia/i18n ] && rm -r /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc/data/opt/Qtopia/i18n; make_ipk_file; rm -rf /opt/xinlan/hardware/adc/arm/.packaging-working-dir/ipk/qpe-adc

