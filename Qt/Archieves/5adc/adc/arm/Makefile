		.ui/release-shared/adc_base.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/adc.o adc.cpp

.obj/release-shared/main.o: main.cpp adc.h \
		../../../../FriendlyARM/mini2440/arm-qtopia/qtopia-2.2.0-FriendlyARM/qtopia/include/qtopia/qpeapplication.h \
		.ui/release-shared/adc_base.h \
		../../../../FriendlyARM/mini2440/arm-qtopia/qtopia-2.2.0-FriendlyARM/qtopia/src/libraries/qtopia/qpeapplication.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/main.o main.cpp

.ui/release-shared/adc_base.h: adc_base.ui 
	$(UIC) adc_base.ui -o .ui/release-shared/adc_base.h

.ui/release-shared/adc_base.cpp: .ui/release-shared/adc_base.h adc_base.ui 
	$(UIC) adc_base.ui -i adc_base.h -o .ui/release-shared/adc_base.cpp

.obj/release-shared/adc_base.o: .ui/release-shared/adc_base.cpp .ui/release-shared/adc_base.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/adc_base.o .ui/release-shared/adc_base.cpp

.obj/release-shared/moc_adc.o: .moc/release-shared/moc_adc.cpp adc.h .ui/release-shared/adc_base.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_adc.o .moc/release-shared/moc_adc.cpp

.obj/release-shared/moc_adc_base.o: .moc/release-shared/moc_adc_base.cpp .ui/release-shared/adc_base.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_adc_base.o .moc/release-shared/moc_adc_base.cpp

.moc/release-shared/moc_adc.cpp: $(MOC) adc.h
	$(MOC) adc.h -o .moc/release-shared/moc_adc.cpp

.moc/release-shared/moc_adc_base.cpp: $(MOC) .ui/release-shared/adc_base.h
	$(MOC) .ui/release-shared/adc_base.h -o .moc/release-shared/moc_adc_base.cpp

####### Install

install_desktop: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/apps/Applications/" || $(MKDIR) "$(INSTALL_ROOT)/apps/Applications/"
	-$(INSTALL_FILE) "adc.desktop" "$(INSTALL_ROOT)/apps/Applications/"


uninstall_desktop: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/apps/Applications/adc.desktop"
	-$(DEL_DIR) "$(INSTALL_ROOT)/apps/Applications/"


install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/bin/" || $(MKDIR) "$(INSTALL_ROOT)/bin/"
	-$(INSTALL_FILE) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/bin/$(QMAKE_TARGET)"
	-arm-linux-strip "$(INSTALL_ROOT)/bin/$(QMAKE_TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/bin/"


install_auto_pics: all 
	@ /opt/FriendlyARM/mini2440/arm-qtopia/qtopia-2.2.0-FriendlyARM/qtopia/bin/installpic "en_US" 14 16 22 $(INSTALL_ROOT)/pics/Games pics/* ;


install: install_desktop install_target install_auto_pics  

uninstall: uninstall_desktop uninstall_target  

