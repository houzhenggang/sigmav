		../../../../FriendlyARM/mini2440/x86-qtopia/qtopia-2.2.0-FriendlyARM/qtopia/include/qtopia/qpeapplication.h \
		.ui/release-shared/your_button_base_form.h \
		../../../../FriendlyARM/mini2440/x86-qtopia/qtopia-2.2.0-FriendlyARM/qtopia/src/libraries/qtopia/qpeapplication.h
