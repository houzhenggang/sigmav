.ui/release-shared/your_button_base_form.h: your_button_base_form.ui 
	$(UIC) your_button_base_form.ui -o .ui/release-shared/your_button_base_form.h

.ui/release-shared/your_button_base_form.cpp: .ui/release-shared/your_button_base_form.h your_button_base_form.ui 
	$(UIC) your_button_base_form.ui -i your_button_base_form.h -o .ui/release-shared/your_button_base_form.cpp

.obj/release-shared/your_button_base_form.o: .ui/release-shared/your_button_base_form.cpp .ui/release-shared/your_button_base_form.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/your_button_base_form.o .ui/release-shared/your_button_base_form.cpp

.obj/release-shared/moc_yourbutton.o: .moc/release-shared/moc_yourbutton.cpp yourbutton.h .ui/release-shared/your_button_base_form.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_yourbutton.o .moc/release-shared/moc_yourbutton.cpp

.obj/release-shared/moc_your_button_base_form.o: .moc/release-shared/moc_your_button_base_form.cpp .ui/release-shared/your_button_base_form.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/release-shared/moc_your_button_base_form.o .moc/release-shared/moc_your_button_base_form.cpp

.moc/release-shared/moc_yourbutton.cpp: $(MOC) yourbutton.h
	$(MOC) yourbutton.h -o .moc/release-shared/moc_yourbutton.cpp

.moc/release-shared/moc_your_button_base_form.cpp: $(MOC) .ui/release-shared/your_button_base_form.h
	$(MOC) .ui/release-shared/your_button_base_form.h -o .moc/release-shared/moc_your_button_base_form.cpp

####### Install

install_desktop: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/apps/Applications/" || $(MKDIR) "$(INSTALL_ROOT)/apps/Applications/"
	-$(INSTALL_FILE) "yourbutton.desktop" "$(INSTALL_ROOT)/apps/Applications/"
	-strip "$(INSTALL_ROOT)/apps/Applications/yourbutton.desktop"


uninstall_desktop: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/apps/Applications/yourbutton.desktop"
	-$(DEL_DIR) "$(INSTALL_ROOT)/apps/Applications/"


install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/bin/" || $(MKDIR) "$(INSTALL_ROOT)/bin/"
	-$(INSTALL_FILE) "$(PWD)/$(QMAKE_TARGET)" "$(INSTALL_ROOT)/bin/$(QMAKE_TARGET)"
	-strip "$(INSTALL_ROOT)/bin/$(QMAKE_TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/bin/"


install_auto_pics: all 
	@ /opt/FriendlyARM/mini2440/x86-qtopia/qtopia-2.2.0-FriendlyARM/qtopia/bin/installpic "en_US" 14 16 22 $(INSTALL_ROOT)/pics/Games pics/* ;


install: install_desktop install_target install_auto_pics  

uninstall: uninstall_desktop uninstall_target  

