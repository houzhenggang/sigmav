unix_mkipk:
	@ if [ -e /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/data ]; then echo "ERROR: /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/data exists!"; exit 1; fi; if [ -e /opt/xinlan/hardware/yourButton/x86/ipkg/qpe-yourbutton__x86.ipk ]; then echo "Package qpe-yourbutton__x86.ipk already exists!"; exit 0; fi; IPK_GROUP_PROJECTS="/opt/xinlan/hardware/yourButton/x86"; for i in $${IPK_GROUP_PROJECTS};do make -C $$i -f Makefile unix_mkipk_deps INSTALL_ROOT=/opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/data/opt/Qtopia || exit $$?; done; make_ipk_file() { cd /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/data; if [ ! -z "$$1" ]; then lang="$$1"; suffix="_i18n_$$lang"; deps="qpe-yourbutton"; if [ "`echo qpe-yourbutton | cut -c -4`" = "qpe-" -a "qpe-yourbutton" != "qpe-taskbar" ]; then deps="$$deps qpe-taskbar_i18n_$$lang"; fi; path="opt/Qtopia/i18n/$$lang"; if [ ! -d "/opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/data/$$path" ]; then return; fi; else suffix=; deps=`cat /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/DEPS`; deps=`echo $$deps | sort -u`; path="opt/Qtopia"; fi; echo "Package: qpe-yourbutton$$suffix" > /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; echo "Maintainer: Unspecified" >> /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; echo "License: GPL" >> /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; echo "Version: " >> /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; echo "Arch: x86" >> /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; echo "Architecture: x86" >> /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; echo "Description: No description written for this package" >> /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; echo -n "Depends: " >> /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; echo $$deps >> /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; echo -n "Files:" >> /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; find $$path -type f -o -type l -printf "/%P " >> /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; if [ ! -x "`which mkfs.jffs2`" ]; then echo -e "\nInstalled-Size: $$(du -h -s $$path | awk '{print $$1}')" >> /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; else echo -e "\nInstalled-Size: $$(mkfs.jffs2 -r $$path | wc -c)" >> /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; fi; echo >> /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control/control; for i in preinst prerm postinst postrm;do export FILE_LOC=/opt/xinlan/hardware/yourButton/x86/qpe-yourbutton.$$i; if [ -e $$FILE_LOC ]; then install -c $$FILE_LOC /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/$$i; fi; done; cd /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton; echo "2.0" > debian-binary; cd /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control; tar --remove-files -cz --owner=0 --group=0 -f /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/control.tar.gz ./control; cd /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/data; tar --remove-files -cz --owner=0 --group=0 -f /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/data.tar.gz $$path; if [ ! -e /opt/xinlan/hardware/yourButton/x86/ipkg ];then mkdir -p /opt/xinlan/hardware/yourButton/x86/ipkg;fi; cd /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton; tar --remove-files -cz --owner=0 --group=0 -f /opt/xinlan/hardware/yourButton/x86/ipkg/qpe-yourbutton__x86$$suffix.ipk ./debian-binary ./control.tar.gz ./data.tar.gz `for i in preinst prerm postinst postrm;do if [ -e $$i ]; then echo $$i; fi; done`; }; LANGUAGES="en_US"; for lang in $${LANGUAGES}; do make_ipk_file $$lang; done; [ -d /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/data/opt/Qtopia/i18n ] && rm -r /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton/data/opt/Qtopia/i18n; make_ipk_file; rm -rf /opt/xinlan/hardware/yourButton/x86/.packaging-working-dir/ipk/qpe-yourbutton

